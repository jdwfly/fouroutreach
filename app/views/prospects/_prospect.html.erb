<div class="panel panel-default">
  <div class="panel-heading">
    <h3 class="panel-title"><%= link_to prospect.name, prospect %></h3>
  </div>
  <div class="panel-body">
    <p>
      <%= prospect.address %><br>
      <%= prospect.city %>, <%= prospect.state %>
    </p>
    <p>
      <%= link_to "edit", edit_prospect_path(prospect), class: "btn btn-primary" %>
      <%= link_to "delete", prospect_url(prospect), method: :delete,
       class: "btn btn-danger" %>
    </p>
  </div>
</div>
