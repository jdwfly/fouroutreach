<div class="col-md-4">
  <div class="box box-default">
    <div class="box-header with-border">
      <h3 class="box-title"><%= link_to prospect.name, prospect %></h3>
    </div>
    <div class="box-body">
      <p>
        <span>Last Contacted: <%= prospect.last_update %></span>
      </p>
      <p>
        <%= prospect.address %><br>
        <%= prospect.city %>, <%= prospect.state %>
      </p>
      <p>
        <%= link_to "edit", edit_prospect_path(prospect), class: "btn btn-primary" %>
        <%= link_to "delete", prospect_url(prospect), method: :delete,
                    class: "btn btn-danger" %>
      </p>
    </div>
  </div>
</div>
